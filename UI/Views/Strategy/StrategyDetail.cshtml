@model Models.Strategy
@{
    ViewBag.Title = "StrategyDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .container1 {
        width: 1200px;
        margin: 0 auto;
    }

    .wrap {
        padding: 20px;
        overflow: hidden;
    }

    .box1 {
        float: left;
        width: 750px;
        box-shadow: 0 0 4px #cecccc;
        -webkit-box-shadow: 0 0 4px #cecccc;
        background-color: #ffffff;
        height: 100%;
    }

    .box2 {
        float: right;
        width: 400px;
        background-color: #ffffff;
        height: 700px;
        box-shadow: 0 0 4px #cecccc;
        -webkit-box-shadow: 0 0 4px #cecccc;
    }

    .T {
        padding: 20px;
        overflow: hidden;
        width: 750px;
        height: 130px;
    }

    .blank {
        height: 20px;
        width: 750px;
        clear: both; /*清除浮动造成的影响*/
        background-color: #F0EFEE;
    }

    .clearfix:after {
        content: '';
        display: table;
        clear: both;
    }

    .clearfix {
        *zoom: 1;
    }
</style>

<header id="head" class="secondary">
    <div class="container">
        <h1>考研攻略</h1>
        <p></p>
    </div>
</header>

<div class="container1">
    <div class="wrap clearfix">
        <div class="box1">

            <div class="T">
                <h3 style="text-align:center; margin:0 auto;font-family:KaiTi;">标题</h3>
                <br />
                <p style="text-align:right;">时间</p>
                <HR style="FILTER: alpha(opacity=100,finishopacity=0,style=3)" width="80%" color=#987cb9 SIZE=3>
            </div>
            <div style="height: 800px;"></div>

        </div>


        <div class="box2">

        </div>
    </div>
</div>